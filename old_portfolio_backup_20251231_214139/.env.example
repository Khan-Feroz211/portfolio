# ENVIRONMENT VARIABLES EXAMPLE
# Copy this to .env file and fill with your actual values
# NEVER commit .env to GitHub!

# Admin emails (use real emails here in your .env file)
PRIMARY_ADMIN_EMAIL=your_real_email@domain.com
BACKUP_ADMIN_EMAIL=your_backup_email@domain.com

# Secret keys (generate new ones for each project)
SECRET_KEY=generate_a_random_string_here
DATABASE_URL=your_database_connection_string

# API Keys (if using external services)
WEATHER_API_KEY=your_weather_api_key_here
EMAIL_PASSWORD=your_email_app_password

# Development settings
DEBUG=False
ENVIRONMENT=production
